{
  "schemaVersion": 1,
  "id": "clojurewrapper",
  "version": "${version}",
  "name": "ClojureWrapper",
  "description": "A wrapper API and loader for the Clojure language",
  "authors": [
    "Luke Bemish"
  ],
  "contact": {
    "sources": "https://github.com/lukebemish/clojurewrapper"
  },
  "license": "LGPL v3",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "clojureloader",
        "value": "com.github.lukebemish.clojurewrapper.init/init"
      }
    ]
  },
  "languageAdapters": {
    "clojureloader": "com.github.lukebemish.clojureloader.ClojureLanguageAdapter"
  },
  "depends": {
    "minecraft": ">=1.18",
    "fabric": "*"
  }
}
